                                  <xs:attribute type="xs:string" name="maxOccurrence" use="optional"/>
                                </xs:extension>
                              </xs:simpleContent>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="Input" maxOccurs="unbounded" minOccurs="0">
